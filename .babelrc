{
  "presets": [
    "env",
    "react"
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "src": "./src",
	      "public": "./src/public/",
	      "components": "./src/components/",
	      "utils": "./src/utils",
	      "services": "./src/services"
      }
    }]
  ]
}